{
  "collectionName": "components_landing_page_layout_buttons",
  "info": {
    "displayName": "Button",
    "description": ""
  },
  "options": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Link": {
      "type": "string",
      "required": true
    },
    "Variant": {
      "type": "enumeration",
      "enum": [
        "Filled",
        "Outline"
      ]
    },
    "Icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "buttonColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
